<%= form_for @song do |s| %>
  <%= s.label :name %>
  <%= s.text_field :name %><br><br>
  <%= s.label :genre %>
  <%= s.number_field :genre_id %><br><br>
  <%= s.label :artist %>
  <%= s.number_field :artist_id%><br><br>
  <%= s.submit %>
<% end %>

